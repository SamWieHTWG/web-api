      <Text Language="de">1 ... MAX(UNS32)</Text>
    </Range>
    <Type>UNS32</Type>
    <MaxNumber>
      <ArrayIndex No="0" Name="hr_data">6</ArrayIndex>
    </MaxNumber>
    <Encrypted>false</Encrypted>
    <ChangeRequiresRestart>false</ChangeRequiresRestart>
    <Help>
      <Link Language="en">https://www.isg-stuttgart.de/fileadmin/kernel/kernel-html/en-GB/index.html#108592523</Link>
      <Link Language="de">https://www.isg-stuttgart.de/fileadmin/kernel/kernel-html/de-DE/index.html#108592523</Link>
    </Help>
  </Parameter>
  <Parameter>
    <Id>P-MANU-00006</Id>
    <Name>hr_data[].hr_aufl_n</Name>
    <ShortDescription>
      <Text Language="en">Handwheel resolution denominator</Text>
      <Text Language="de">Nenner der Handradauflösung</Text>
    </ShortDescription>
    <LongDescription>
      <Text Language="en">The parameter defines the handwheel resolution denominator.</Text>
      <Text Language="de">Der Parameter definiert den Nenner der Handradauflösung.</Text>
    </LongDescription>
    <Quantity>REVOLUTION</Quantity>
    <Units>
      <Unit>UNIT_REVOLUTION</Unit>
    </Units>
