      <P-RTCF-00009.4 Value="4000" />
      <!-- Priority of the thread, depending on the used real time system - "LOW" -->
      <P-RTCF-00010.4 Value="32" />
      <!-- Error on overflow -->
      <P-RTCF-00011.4 Value="0" />
      <!-- CPU affinity - number of the cpu -->
      <P-RTCF-00015.4 Value="1" />
      <Function>
