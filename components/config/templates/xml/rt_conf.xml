        <P-RTCF-00013.2.1 Value="1" />
        <!-- Trace information -->
        <P-RTCF-00014.2.1 Value="0" />
      </Function>
    </Thread>
    <Thread>
      <!-- Name of the thread -->
      <P-RTCF-00008.3 Value="CNC_SYS" />
      <!-- Cycle time of the thread in mikro s -->
      <P-RTCF-00009.3 Value="4000" />
      <!-- Priority of the thread, depending on the used real time system - "LOW" -->
      <P-RTCF-00010.3 Value="31" />
      <!-- Error on overflow -->
      <P-RTCF-00011.3 Value="0" />
